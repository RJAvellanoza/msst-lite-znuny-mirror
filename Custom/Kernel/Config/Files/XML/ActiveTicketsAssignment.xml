<?xml version="1.0" encoding="utf-8"?>
<otrs_config version="2.0" init="Framework">

    <!-- Frontend Module Registration for Active Tickets Assignment -->
    <Setting Name="Frontend::Module###AgentActiveTicketsAssignment" Required="0" Valid="1">
        <Description Translatable="1">Frontend module registration for Active Tickets Assignment Report.</Description>
        <Navigation>Frontend::Agent::ModuleRegistration</Navigation>
        <Value>
            <Item ValueType="FrontendRegistration">
                <Hash>
                    <Item Key="Group">
                        <Array>
                            <Item>users</Item>
                        </Array>
                    </Item>
                    <Item Key="GroupRo">
                        <Array>
                            <Item>users</Item>
                        </Array>
                    </Item>
                    <Item Key="Description" Translatable="1">View active tickets assignment report by priority.</Item>
                    <Item Key="Title" Translatable="1">Active Tickets Assignment Report</Item>
                    <Item Key="NavBarName">Reports</Item>
                    <Item Key="Module">Kernel::Modules::AgentActiveTicketsAssignment</Item>
                </Hash>
            </Item>
        </Value>
    </Setting>

    <!-- Frontend Module Registration for Average Backlog -->
    <Setting Name="Frontend::Module###AgentAverageBacklog" Required="0" Valid="1">
        <Description Translatable="1">Frontend module registration for Active Tickets Average Backlog.</Description>
        <Navigation>Frontend::Agent::ModuleRegistration</Navigation>
        <Value>
            <Item ValueType="FrontendRegistration">
                <Hash>
                    <Item Key="Group">
                        <Array>
                            <Item>users</Item>
                        </Array>
                    </Item>
                    <Item Key="GroupRo">
                        <Array>
                            <Item>users</Item>
                        </Array>
                    </Item>
                    <Item Key="Description" Translatable="1">View average backlog of active tickets by priority.</Item>
                    <Item Key="Title" Translatable="1">Active Tickets Average Backlog</Item>
                    <Item Key="NavBarName">Reports</Item>
                    <Item Key="Module">Kernel::Modules::AgentAverageBacklog</Item>
                </Hash>
            </Item>
        </Value>
    </Setting>

</otrs_config>
